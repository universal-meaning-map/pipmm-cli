{
  "name": "pipmm",
  "description": "Set of tools meant to play with the underlying assumptions of `Interplanetary mind-map`",
  "version": "0.0.19",
  "author": "xavivives",
  "bin": {
    "pipmm": "./bin/run"
  },
  "bugs": "https://github.com/interplanetarymindmap/pipmm-cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.16",
    "@types/ini": "^1.3.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.58",
    "@types/node-ipc": "^9.1.3",
    "@types/ws": "^8.2.0",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^9.0.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/client",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/interplanetarymindmap/pipmm-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pipmm",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "interplanetarymindmap/pipmm-cli",
  "scripts": {
    "run": "node bin/run watch",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@ipld/dag-json": "^8.0.0",
    "@ipld/schema-validation": "^1.1.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@pinecone-database/pinecone": "^0.1.5",
    "axios": "^0.24.0",
    "chokidar": "^3.5.2",
    "cli-ux": "^5.5.1",
    "connect": "^3.7.0",
    "gray-matter": "^4.0.2",
    "hnswlib-node": "^1.4.2",
    "ipfs-core": "^0.10.4",
    "ipld-schema": "^1.1.6",
    "langchain": "^0.0.74",
    "multiformats": "^9.4.5",
    "openai": "^3.2.1",
    "pipmm-server": "^1.0.0",
    "serve-static": "^1.14.1",
    "string-template": "^1.0.0",
    "ws": "^7.5.4",
    "yaml": "^2.3.0"
  }
}
